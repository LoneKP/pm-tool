<h1>New project</h1>


<div class ="row">
	<div class="col-xs-12">
		<%= form_for(@project, :html => {class: "form-horizontal", role: "form"}) do |f| %>
				<div class="form-group">
					<div class="control-label col-sm-2">

			<%= f.label :harvest_project_id %>
					</div>
					<div class="col-sm-8">
			<%= f.text_field :harvest_project_id, class: "form-control", placeholder: "Enter ID", autofocus: true %>
					</div>
				</div>
					<div class="form-group">
			<div class="col-sm-offset-2 col-sm-10">
				<%= f.submit( class: 'btn btn-light') %>

		</div>
		
	</div>

		<% end %>

			<p>
			<%= link_to 'Back to projects', projects_url %>
			</p>
	</div>
</div>



